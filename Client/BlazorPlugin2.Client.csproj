    <ItemGroup>
        <ProjectReference Include="..\RazorClassLibrary1\RazorClassLibrary1.csproj"/>
        <ProjectReference Include="..\Shared\BlazorPlugin2.Shared.csproj"/>
    </ItemGroup>

